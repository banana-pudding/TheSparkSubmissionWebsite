<% if (file.isNewSubmission || isAll) { %> <% if (file.isReviewed) { %> <% if (file.isRejected) { %>
<span class="badge rounded-pill bg-danger">Rejected</span>
<% } else { %>
<span class="badge rounded-pill bg-success">Accepted</span>
<% } %> <% } else { %>
<span class="badge rounded-pill bg-primary">Unreviewed</span>
<% } %> <% } %> <% if (file.isPrinted || isAll) { %> <% if (file.pickupLocation == "Willis Library") { %>
<span class="badge rounded-pill bg-info">Willis</span>
<% } else { %>
<span class="badge rounded-pill bg-info">DP</span>
<% } %> <% } else { %> <% if (file.printLocation == "Willis Library") { %>
<span class="badge rounded-pill bg-info">Willis</span>
<% } else { %>
<span class="badge rounded-pill bg-info">DP</span>
<% } %> <% } %> <%if (!file.isPickedUp) {%> <% if (file.dateOfConfiscation != 'Not yet sent') {%>
<span class="badge rounded-pill bg-danger">Reposessed</span>
<% } else if (file.dateOfSecondWarning != 'Not yet sent') { %>
<span class="badge rounded-pill bg-warning">Final Warning</span>
<% } else if (file.dateOfFirstWarning != 'Not yet sent') {%>
<span class="badge rounded-pill bg-warning">First Warning</span>
<% } %> <%}%> <% if (file.isPendingDelete) { %>
<span class="badge rounded-pill bg-danger">Pending Delete</span>
