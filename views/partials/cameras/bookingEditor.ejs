<div class="card-body" id="editor-controls" style="display: none;">
    <form action="/bookings/manualbooking" method="POST">
        <div class="form-group">
            <label for="availableCameras">
                Camera
            </label>
            <select class="form-control" id="availableCameras" name="camera">
                <%cameras.forEach(function (camera) { %>
                <option><%=camera%></option>
                <%})%>
            </select>
        </div>

        <div class="form-group">
            <label for="availableCameras">
                Lens 1
            </label>
            <select class="form-control" id="availableLenses1" name="lens1">
                <%lenses.forEach(function (lens) { %>
                <option><%=lens%></option>
                <%})%>
            </select>
        </div>

        <div class="form-group">
            <label for="availableCameras">
                Lens 2
            </label>
            <select class="form-control" id="availableLenses2" name="lens2">
                <%lenses.forEach(function (lens) { %>
                <option><%=lens%></option>
                <%})%>
                <option>None</option>
            </select>
        </div>

        <div class="form-group">
            <label>Start Date</label>
            <input
                type="date"
                id="startDate"
                name="startDate"
                class="form-control"
            />
        </div>

        <div class="form-group">
            <label>End Date</label>
            <input
                type="date"
                id="endDate"
                name="endDate"
                class="form-control"
            />
        </div>

        <button class="btn btn-block btn-success" type="submit">Submit</button>
    </form>
</div>
