<div class="form-group single-print">
  <div class="form-group row">
    <div class="custom-file" id="customFile" lang="es">
      <input name="uploadedFile" type="file" class="custom-file-input" id="exampleInputFile" aria-describedby="fileHelp"
        required>
      <label class="custom-file-label" for="exampleInputFile">
        Select file...
      </label>
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-3">Material</label>
    <select name="material" class="form-control col-sm-9" required>
      <option value="nopref">No preference</option>
      <option value="pla">PLA</option>
      <option value="abs">ABS</option>
      <option value="other">Other (requires prior approval)</option>
    </select>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-3">Infill</label>
    <input name="infill" type="text" class="form-control col-sm-9" value="12.5%" required>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-3">Color</label>
    <input name="color" type="text" class="form-control col-sm-9" value="No preference" required>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-3">Copies</label>
    <input name="copies" type="number" class="form-control col-sm-9" value="1" required>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-3">Pickup at</label>
    <select name="pickup" class="form-control col-sm-9" required>
      <option value="willis">Willis Library</option>
      <option value="dp">Discovery Park</option>
    </select>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-3" for="notes">Additional Notes</label>
    <textarea name="notes" class="form-control col-sm-9" rows="2"></textarea>
  </div>

  <hr>

</div>