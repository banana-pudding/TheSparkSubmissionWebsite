<% for (var i = 0; i < submissions.length; i++) { %>
<li class="list-group-item">
    <button type="button" class="btn btn-warning btn-sm float-right btn-waive" itemID="<%=submissions[i][0]%>">
        Waive
    </button>
    <div>
        <% for (var j = 0; j < submissions[i][1].length; j++) { %>
        <a href="/prints/preview/?fileID=<%=submissions[i][2][j]%>"><%= submissions[i][1][j]%></a><br />
        <% } %>
    </div>
</li>
<% } %>
